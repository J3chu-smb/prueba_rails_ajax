<p id="notice"><%= notice %></p>

<p>
  <strong>Name:</strong>
  <%= @company.name %>
</p>

<%= render 'complaints/form', complaint:@complaint %>
<ul id="admin">
<% @company.complaints.order(created_at: :desc).each do |complaint| %>
  <li><%= complaint.content %>
  <%= complaint.user.name %></li>
<% end %>
</ul>
<%= link_to 'Edit', edit_company_path(@company), class:'btn btn-info btn-sm' %>
